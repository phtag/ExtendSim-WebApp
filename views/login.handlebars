<div id="home">
    <div class="container">
        <div class="row">
            <header id="ExtendSim-header">
            </header>
        </div>
        <div class="row">
            <div class="col-8 offset-2">
                <h2>ExtendSim Web Simulation Login Page</h2>
                <form class="clearfix mb-4" action="POST">
                    <div class="form-group">
                        <label for="example-text">Username</label>
                        <input type="text" id="username-text" class="form-control" aria-describedby="example-text" placeholder="Enter username">
                    </div>
                    <div class="form-group">
                        <label for="password-text">Password</label>
                        <input type="password" id="password-text" class="form-control" aria-describedby="password-text"></textarea>
                    </div>
                    <button id="submit-login-info" class="btn btn-primary float-left">Submit</button>
                </form>
            </div>
        </div>
    </div>
</div>

<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="/js/index.js"></script>
